    android:keyHeight="10%p"
    android:horizontalGap="1px" >

    <Row>
        <Key android:codes="0"    android:keyLabel="f(x)" android:popupKeyboard="@xml/popup_functions"  android:horizontalGap="0%p" android:keyEdgeFlags="left" />
        <Key android:codes="40"    android:keyLabel="(" android:horizontalGap="4%p" />
        <Key android:codes="41"    android:keyLabel=")" android:horizontalGap="2%p" />
        <Key android:codes="49"    android:keyLabel="1" android:horizontalGap="4%p" />
        <Key android:codes="50"    android:keyLabel="2" android:horizontalGap="2%p" />
        <Key android:codes="51"    android:keyLabel="3" android:horizontalGap="2%p" />
        <Key android:codes="120"   android:keyLabel="x" android:horizontalGap="4%p" />
        <Key android:codes="121"   android:keyLabel="y" android:horizontalGap="2%p" android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:codes="32"    android:keyIcon="@drawable/sym_keyboard_space" android:horizontalGap="0%p"  />
        <Key android:codes="43"    android:keyLabel="+" android:horizontalGap="4%p" />
        <Key android:codes="45"    android:keyLabel="-" android:horizontalGap="2%p" />
        <Key android:codes="52"    android:keyLabel="4" android:horizontalGap="4%p" />
        <Key android:codes="53"    android:keyLabel="5" android:horizontalGap="2%p" />
        <Key android:codes="54"    android:keyLabel="6" android:horizontalGap="2%p" />
        <Key android:codes="48"    android:keyLabel="0" android:horizontalGap="2%p" />
        <Key android:codes="-5"    android:keyIcon="@drawable/sym_keyboard_delete_lxx_dark" android:horizontalGap="4%p" android:isRepeatable="true" />
    </Row>
    <Row>
        <Key android:codes="55000" android:keyIcon="@drawable/sym_keyboard_previous_lxx_dark" android:horizontalGap="0%p" />
        <Key android:codes="42"    android:keyLabel="*" android:horizontalGap="4%p" />
        <Key android:codes="47"    android:keyLabel="/" android:horizontalGap="2%p"/>
        <Key android:codes="55"    android:keyLabel="7" android:horizontalGap="4%p"/>
        <Key android:codes="56"    android:keyLabel="8" android:horizontalGap="2%p" />
        <Key android:codes="57"    android:keyLabel="9" android:horizontalGap="2%p" />
        <Key android:codes="46"    android:keyLabel="." android:horizontalGap="2%p" />
        <Key android:codes="55005" android:keyIcon="@drawable/sym_keyboard_next_lxx_dark" android:horizontalGap="4%p" />
